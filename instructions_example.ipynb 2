    "from finance_python.stock import stock\n",
    "from finance_python.industry import industry\n",
    "import sys, os, time\n",
    "import numpy as np\n",
    "import pandas\n",
    "from datetime import datetime, date, timedelta\n",
    "from pandas import DataFrame"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "symbols = ['SHOP.TO', 'KXS.TO', 'OTEX.TO']\n",
    "start = date.today() - timedelta(days=365)\n",
    "end = date.today()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "stock_loop = lambda x: stock(x, start, end)\n",
    "stat = lambda x: stocks[x].stats()\n",
    "bal = lambda x: stocks[x].balance()\n",
    "fin = lambda x: stocks[x].financial()\n",
    "ca = lambda x: stocks[x].cash()\n",
    "an = lambda x: stocks[x].analyze()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "19.74023127555847\n"
     ]
    }
   ],
   "source": [
    "timer_start = time.time()\n",
    "\n",
    "stocks = list(map(stock_loop, symbols))\n",
    "\n",
    "timer_end = time.time()\n",
    "print(timer_end - timer_start)"
   ]
  },
  {
   "cell_type": "code",
